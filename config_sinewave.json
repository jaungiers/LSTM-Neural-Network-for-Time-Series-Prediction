{
	"data": {
			"filename": "sinewave.csv",
			"columns": [
					"sinewave"
			],
			"sequence_length": 50,
			"train_test_split": 0.8,
			"normalise": false
	},
	"training": {
			"epochs": 2,
			"batch_size": 32
	}, "model": {
			"loss": "mse",
			"optimizer": "adam",
			"layers": [
				{
				"type": "lstm",
				"neurons": 50,
				"input_timesteps": 49,
				"input_dim": 1,
				"return_seq": true
				}, {
				"type": "dropout",
				"rate": 0.05
				}, {
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
				}, {
				"type": "dropout",
				"rate": 0.05
				}, {
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
				}
		   	]
	} 
}
